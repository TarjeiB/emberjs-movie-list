<div>
  <div>
    {{input type="text" value=title placeholder="Title" enter="search"}}
    <button {{action "search" title}}>Search</button>
  </div>
{{#each model as |movie|}}
  <p> {{movie.Title}} </p>
  <p>Year: {{movie.Year}}</p>
  <p>IMDB rating {{movie.imdbRating}}</p>
  <img src="{{movie.Poster}}" />
{{/each}}
</div>
